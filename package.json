{
    "name": "elmer-virtual-dom",
    "version": "2.0.1",
    "description": "htmlParse and virtualDom diff",
    "typings": "types/index.d.ts",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "scripts": {
        "testReport": "mocha ./test/index.test.js ./test/**/*.test.js --reporter mochawesome",
        "test": "mocha --reporter mochawesome",
        "cover": "nyc --reporter=html mocha",
        "lib": "tsc -w",
        "build": "tsc"
    },
    "keywords": [
        "javascript",
        "typescript"
    ],
    "author": "250933400@qq.com",
    "license": "ISC",
    "nyc": {
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    },
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "babel-loader": "^7.1.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "mochawesome": "^4.1.0",
        "nyc": "^14.1.1",
        "tslint": "^5.11.0",
        "typescript": "^3.6.4",
        "jsdom": "^15.2.0",
        "mocha": "^6.2.1",
        "chai": "^4.2.0",
        "ts-node": "^8.10.2"
    },
    "dependencies": {
        "@abraham/reflection": "^0.4.2",
        "@types/core-js": "^2.5.0",
        "ajv": "5.0.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "chalk": "^2.4.1",
        "core-js": "^2.5.7",
        "decorator": "^0.0.1",
        "elmer-common": "^0.1.10",
        "eslint-plugin-typescript": "^0.12.0",
        "reflect-metadata": "^0.1.12",
        "typescript-eslint-parser": "^18.0.0",
        "zone.js": "^0.8.26"
    }
}